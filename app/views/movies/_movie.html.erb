<div id="<%= dom_id movie %>">
  <div>
    <strong>Title:</strong>
    <%= movie.title %>
  </div>

  <div>
    <strong>Tmdb:</strong>
    <%= movie.tmdb_id %>
  </div>

  <div>
    <strong>Overview:</strong>
    <%= movie.overview %>
  </div>

  <div>
    <strong>Poster path:</strong>
    <%= movie.poster_path %>
  </div>

  <div>
    <strong>Release year:</strong>
    <%= movie.release_year %>
  </div>

</div>
